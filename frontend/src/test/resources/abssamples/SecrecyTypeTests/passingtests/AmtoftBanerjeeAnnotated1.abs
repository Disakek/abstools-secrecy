module NewObjectsExample;

interface AmtoftBanerjeeI {
    [Secrecy: High] Int getQ();
    Unit setQ([Secrecy: High] Int n);
}

class AmtoftBanerjee implements AmtoftBanerjeeI {

    [Secrecy: High] Int secret = 0;
    [Secrecy: High] Int q = 0;
    [Secrecy: High] Int z = 0;

    [Secrecy: High]  Int getQ() {
        return this.q;
    }

    Unit setQ([Secrecy: High] Int n) {
        this.q = n;
    }

    Unit m_1() {
        AmtoftBanerjeeI x1;
        AmtoftBanerjeeI x2 = new AmtoftBanerjee();
        x1 = x2;
        x1.setQ(secret);
        z = x2.getQ();
    }

    Unit m_2() {
        AmtoftBanerjeeI x1 = new AmtoftBanerjee();
        AmtoftBanerjeeI x2 = new AmtoftBanerjee();
        x1.setQ(secret);
        z = x2.getQ();
    }
}

