module SumExample;

class SumExample(List<Int> in_values) {

    [Secrecy: High] List<Int> values = in_values;
    
    Int getSum() {
        Int sum = 0;
        Int i = 0;

        //This is discussable should only the list reference be High or also each element?
        //Handling depends on that.
        while (i < length(values)) {
            sum = sum + nth(values, i);
            i = i+1;
        }

        return sum;
    }
}